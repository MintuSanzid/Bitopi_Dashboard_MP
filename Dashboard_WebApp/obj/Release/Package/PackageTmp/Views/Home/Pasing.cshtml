
@{
    ViewBag.Title = "Pasing";
    Layout = "~/Views/Shared/_LayoutConfigurationHRF.cshtml";
}

<h2>Pasing</h2>

  <body ng-app="app" ng-controller="TodoController">
    <h1>Todos</h1>
    <h4>{{todos.length}} total</h4>
    <ul>
      <li ng-repeat="todo in filteredTodos">{{todo.text}}</li>
    </ul>
    <pagination
      ng-model="currentPage"
      total-items="todos.length"
      max-size="maxSize"
      boundary-links="true">
    </pagination>
  </body>
